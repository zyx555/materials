/* 水平间距 */
.container {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.item {
  width: 86%;
  margin-top: 12px;
  border-radius: 6%;
  background-color: #FFFFFF;
}
.priceName {
  color: #2784FF;
  padding: 10px;
}
.time {
  font-size: 12px;
  color: #A4ADB3;
  padding: 10px;
}
.buttonBox {
  display: flex;
  justify-content: space-around;
  margin-top: 15px;
  margin-bottom: 10px;
}
.check, .download {
  width: 52px;
  height: 24px;
  font-size: 12px;
  line-height: 24px;
  color: #FFFFFF;
  background-color: #2784FF;
}
.delete {
  width: 52px;
  height: 24px;
  font-size: 12px;
  line-height: 24px;
  color: #FFFFFF;
  background-color: #A4ADB3;
}

