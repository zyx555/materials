.container {
  padding: 20px;
  font-size: 14px;
  line-height: 24px;
  margin-top: 57px;
}
.register {
  font-weight: 500;
  font-size: 28px;
  text-align: center;
}
.registerBox {
  display: flex;
  justify-content: center;
  padding: 15px;
}
.accountNumber, .password {
  width: 255px;
  height: 36px;
  position: relative;
  font-size: 14px;
  color: #888888;
  padding: 4px;
  margin-top: 21px;
  background-color: white;
  border-radius: 13px;
}
.checkText {
  font-size: 10px;
  padding: 10px;
}
.checkBox {
  -webkit-transform: scale(0.6);
          transform: scale(0.6);
}
.registerButton {
  height: 44px;
  background-color: #2784FF;
  color: #FFFFFF;
  line-height: 44px;
  font-size: 15px;
  margin-top: 21px;
  border-radius: 13px;
}
.forgetPassword {
  font-size: 12px;
  color: #2784FF;
  display: flex;
  justify-content: flex-end;
  padding: 10px;
}
.psd-eye-active {
  width: 45rpx;
  height: 45rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  position: absolute;
  right: 143rpx;
  top: 28%;
  z-index: 10;
}
